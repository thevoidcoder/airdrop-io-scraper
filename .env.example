# Telegram Bot Configuration
# Get your bot token from @BotFather on Telegram
TELEGRAM_BOT_TOKEN=your_bot_token_here

# Chat ID of the Telegram group
TELEGRAM_CHAT_ID=your_chat_id_here

# Topic IDs for different airdrop categories
# Topic ID for Hot Airdrops
TELEGRAM_TOPIC_HOT=hot_topic_id_here

# Topic ID for Latest Airdrops
TELEGRAM_TOPIC_LATEST=latest_topic_id_here

# Topic ID for Updated Airdrops
TELEGRAM_TOPIC_UPDATED=updated_topic_id_here
